{
    "name": "salsassoc/api",
    "description": "API Slim Framework for Salsassoc",
    "type": "project",
    "require": {
        "slim/slim": "^4.10",
        "slim/psr7": "^1.6",
        "monolog/monolog": "^2.9",
        "vlucas/phpdotenv": "^5.5",
        "php-di/php-di": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
