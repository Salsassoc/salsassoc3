{
  "name": "salsassoc-frontend",
  "version": "3.0.0",
  "description": "Frontend for Salsassoc using React and Ant Design",
  "type": "module",
  "main": "index.js",
  "private": true,
  "homepage": "/",
  "scripts": {
    "preinstall": "npm install --package-lock-only --ignore-scripts && npx npm-force-resolutions",
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build && npm run build-rename && npm run fix-index-html",
    "build-rename": "npm run build-rename-js && npm run build-rename-css",
    "build-rename-js": "renamer --find \"/\\.\\w+/\" --replace \"\" ./build/static/js/*",
    "build-rename-css": "renamer --find \"/\\.\\w+/\" --replace \"\" ./build/static/css/*",
    "fix-index-html": "npm run fix-main-js-index-html && npm run fix-main-css-index-html",
    "fix-main-js-index-html": "sed -i 's/main\\.[a-z0-9]*\\.js/main.js/g' ./build/index.html",
    "fix-main-css-index-html": "sed -i 's/main\\.[a-z0-9]*\\.css/main.css/g' ./build/index.html",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-3.0-only",
  "dependencies": {
    "@ant-design/charts": "^2.6.7",
    "@ant-design/pro-layout": "^7.22.7",
    "antd": "^6.1.3",
    "buffer": "^6.0.3",
    "i18next": "^25.7.3",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0",
    "react-helmet": "^6.1.0",
    "react-i18next": "^16.5.0"
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "react-scripts": "latest",
    "renamer": "^5.0.2"
  },
  "engines": {
    "npm": ">=8.6.0",
    "node": ">=18.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
